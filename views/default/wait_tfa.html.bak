{{extend 'layout.html'}}
<style>
circle.node {
  stroke: #fff;
  stroke-width: 1.5px;
}

line.link {
  stroke: #999;
  stroke-opacity: .6;
}
</style>
<script src="{{=URL('static','js/d3.min.js')}}"></script>
<script src="{{=URL('static','js/logo.js')}}"></script>
<INPUT type="hidden" VALUE="Back" onClick="history.go(-1);"> 
{{if sched_var!="COMPLETED":}}

<center>This page will be updated every 10 sec</center>
<script>
    function ReloadPage() { 
location.reload();
};

$(window).load(function() {
setTimeout("ReloadPage()", 10000);
});
</script>
{{pass}}
{{if sched_var=="QUEUED":}}
<div class="progress center-block" style="width: 1170px">
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 25%">
        QUEUED
    </div>
</div>
{{pass}}

{{if sched_var=="ASSIGNED":}}
    
<div class="progress center-block" style="width: 1170px">
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 50%">
        ASSIGNED
    </div>
</div>
